  <section
    class="section process-section-home-page"
    data-bg-img="{{ "/images/process-section-bg.webp" | url }}"
  >
    <div class="container process-container">
      <div class="process-img-container">
        <img
          class="border-radius-shadow-img"
          src="/images/kiss-wedding.png"
