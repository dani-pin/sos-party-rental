---
page_id: home
---

{% extends "layouts/base.njk" %}

{% block preloads %}
  <link
    rel="preload"
    href="/fonts/URW-Americana-W01-Regular.woff2"
    as="font"
    crossorigin
  />
  <link
    rel="preload"
    href="/fonts/montserrat-v29-latin-regular.woff2"
    as="font"
    crossorigin
  />
  <link
    rel="preload"
    href="/fonts/montserrat-v29-latin-500.woff2"
    as="font"
    crossorigin
  />
{% endblock preloads %}

{% block head %}
  <!-- Swiper -->
  <link
    rel="stylesheet"
    href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css"
    media="print"
    onload="this.media='all'; this.onload=null;"
  />
  <script
    src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"
    defer
  ></script>

  <!-- AOS -->
  <link
    href="https://unpkg.com/aos@2.3.1/dist/aos.css"
    rel="stylesheet"
    media="print"
    onload="this.media='all'; this.onload=null;"
  />
  <script src="https://unpkg.com/aos@2.3.1/dist/aos.js" defer></script>
  <script>
    window.addEventListener('DOMContentLoaded', () => {
      AOS.init()
    })
  </script>
{% endblock head %}

{% block assets %}
  <style>
    /* Navbar */
    .site-nav-primary {
      border-bottom: 0.5px var(--beige-100) solid;
      background-color: var(--brown-500);
    }

    /* Hero section */
    .hero {
      background-color: var(--brown-500);
      background-repeat: no-repeat;
      background-size: cover;
      min-height: calc(100vh - 86.8px);
      background-position: center;
      display: flex;
      align-items: center;
      justify-content: center;
      padding-block: 3rem;
      overflow: hidden;
      padding-top: 220px;
      position: relative;
    }

    .hero-info {
      max-width: 800px;
      text-align: center;
      z-index: 10;
    }

    .hero-subheading {
      margin-top: 1rem;
      margin-bottom: 1.5rem;
      max-width: 570px;
      margin-inline: auto;
    }

    @media only screen and (max-width: 767px) {
      .hero-heading {
        font-size: 2.5rem;
      }
    }

    /* Hero disco balls */
    .hero-ball-container {
      position: absolute;
    }

    .hero-ball-container .line {
      position: absolute;
      bottom: calc(100% - 5px);
      left: 50%;
      transform: translateX(-50%);
      width: 1px;
      height: 100vh;
    }

    .hero-ball-1-container {
      bottom: 61%;
      left: 0;
      z-index: 10;
    }

    .hero-ball-2-container {
      bottom: 0;
      left: 6.6%;
      z-index: 9;
    }

    .hero-ball-3-container {
      bottom: 45%;
      left: 15.9%;
      z-index: 8;
    }

    .hero-ball-4-container {
      top: 0%;
      left: 31%;
      z-index: 7;
    }

    .hero-ball-5-container {
      bottom: 66%;
      left: 42%;
      z-index: 6;
    }

    .hero-ball-6-container {
      bottom: 74%;
      left: 62.3%;
      z-index: 5;
    }

    .hero-ball-7-container {
      bottom: 1%;
      right: 22%;
      z-index: 4;
    }

    .hero-ball-8-container {
      bottom: 30%;
      right: 13%;
      z-index: 3;
    }

    .hero-ball-9-container {
      top: 0;
      right: 1%;
      z-index: 2;
    }

    .hero-ball-10-container {
      bottom: 4%;
      right: 0;
      z-index: 1;
    }

    @media (max-width: 1530px) {
      .hero-ball-3-container,
      .hero-ball-8-container {
        display: none;
      }

      .hero-ball-7-container {
        right: 12%;
      }
    }

    @media (max-width: 1210px) {
      .hero-ball-2-container {
        left: 3%;
      }

      .hero-ball-4-container {
        left: 25%;
      }

      .hero-ball-5-container {
        left: 39%;
      }
    }

    @media (max-width: 1945px) {
      .hero-ball-10-container {
        display: none;
      }

      .hero-ball-7-container {
        right: 3%;
      }

      .hero-ball-9-container {
        left: 86%;
        right: auto;
      }

      .hero-ball-2-container {
        left: -8%;
      }
    }

    @media (max-width: 830px) {
      .hero-ball-2-container,
      .hero-ball-7-container {
        display: none;
      }

      .hero-ball-1-container {
        right: 80%;
      }

      .hero-ball-4-container {
        left: 21%;
      }

      .hero-ball-5-container {
        left: 36%;
      }

      .hero-ball-6-container {
        left: 66%;
      }

      .hero-ball-9-container {
        left: 80%;
      }
    }

    @media (max-width: 680px) {
      .hero-ball-4-container {
        left: 17%;
      }

      .hero-ball-5-container {
        left: 32%;
      }

      .hero-ball-6-container {
        left: 69%;
      }

      .hero-ball-9-container {
        left: 85%;
      }
    }

    @media (max-width: 480px) {
      .hero-ball-1-container,
      .hero-ball-9-container {
        display: none;
      }

      .hero-ball-4-container {
        left: -2%;
      }

      .hero-ball-5-container {
        left: 25%;
        bottom: unset;
        top: 7%;
      }

      .hero-ball-6-container {
        left: 77%;
      }

      .hero {
        padding-top: 240px;
      }
    }

    @media (max-width: 370px) {
      .hero-ball-4-container,
      .hero-ball-6-container {
        display: none;
      }

      .hero-ball-5-container {
        left: 21%;
      }
    }
  </style>
  <link
    rel="stylesheet"
    href="/styles/index.css"
    media="print"
    onload="this.media='all'; this.onload=null;"
  />
{% endblock assets %}

