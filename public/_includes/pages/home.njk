          eleventy:widths="201,450,700,900,auto"
        />
        <div class="overlapping-img">
          <img
            class="border-radius-shadow-img"
            src="/images/ceremony-room.png"
            alt="{{ process.img2 }}"
            sizes="(min-width: 1163px) 228px, (min-width: 1008px) 20vw, (min-width: 532px) 177px, 35vw"
            eleventy:widths="102,227,454,auto"
          />
        </div>
      </div>

      <div class="process-info" data-aos="fade-left" data-aos-duration="900">
        <p class="subheading section-subheading-title">
          {{ process.subheading }}
        </p>
        <h2 class="h2">{{ process.heading }}</h2>
        <div class="process-steps">
          <div class="step-1">
            <div class="step-number process-step-number">01</div>
            <div class="process-step-description step-one">
              {{ process.step1 }}
            </div>
          </div>

          <div class="step-2">
            <div class="step-number process-step-number">02</div>
            <div class="process-step-description">{{ process.step2 }}</div>
          </div>

          <div class="step-3">
